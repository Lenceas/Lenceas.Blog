{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "AppSettings": {
    "MemoryCachingAOP": {
      "Enabled": true
    },
    "RedisCachingAOP": {
      "Enabled": false
    },
    "LogAOP": {
      "Enabled": false
    },
    "SeedDB": true,
    "SeedDBData": true
  },
  "Startup": {
    "Cors": {
      "PolicyName": "CorsIpAccess", //策略名称
      "EnableAllIPs": false, //当为true时，开放所有IP均可访问。
      // 支持多个域名端口，注意端口号后不要带/斜杆：比如localhost:8080/，是错的
      // 注意，http://127.0.0.1:8080 和 http://localhost:8080 是不一样的
      "IPs": "http://127.0.0.1:8080,http://localhost:8080"
    },
    "ApiName": "Lenceas.Core.Api"
  },
  "Audience": {
    "Secret": "", //不要太短，16位+
    "SecretFile": "C:\\my-file\\lenceas.core.audience.secret.txt", //安全。内容就是Secret
    "Expires": 15, // 请求过期时间,单位/分钟
    "Issuer": "Lenceas.Core",
    "Audience": "ljs"
  }
}
